    

def main():
    
    while True:
        x=str(input("Write something (quit ends): "))
        tester(x)
        if (len(x)<=10) and (x!='quit'):
            y=tester()
            print(y)
        
        elif (len(x)>=10) and ('X'in x):
            print(x)
            print('x spotted!')
            
            
        elif (len(x)>=10) :
            print(x)
            
        
        elif x=='quit':
            exit()
    
    

if __name__ == "__main__":
    main()
